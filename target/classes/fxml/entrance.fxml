<?xml version="1.0" encoding="UTF-8"?>

<?import java.net.URL?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>

<?import javafx.scene.layout.Pane?>
<Pane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.EntranceView">

    <stylesheets>
        <URL value="@../CSS/main.css"/>
    </stylesheets>
    <children>
        <Button layoutX="100.0" layoutY="326.0" mnemonicParsing="false" prefHeight="48.0" prefWidth="206.0"
                text="Sign Up" fx:id="signUpButton" onMouseClicked="#signUpClicked"/>
        <Button layoutX="397.0" layoutY="326.0" mnemonicParsing="false" prefHeight="48.0" prefWidth="206.0" text="Exit"
                fx:id="exitButton" onMouseClicked="#exitClicked"/>
        <Button layoutX="700.0" layoutY="326.0" mnemonicParsing="false" prefHeight="48.0" prefWidth="206.0" text="Login"
                fx:id="loginButton" onMouseClicked="#loginClicked"/>
    </children>
</Pane>
