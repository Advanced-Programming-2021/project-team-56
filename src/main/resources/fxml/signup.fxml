<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="700.0" prefWidth="1000.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.SignUpView">
   <children>
      <Label alignment="CENTER" layoutX="390.0" layoutY="13.0" prefHeight="52.0" prefWidth="186.0" text="Sign Up Men" />
      <Label layoutX="332.0" layoutY="269.0" prefHeight="30.0" prefWidth="107.0" text="username:" />
      <TextField layoutX="439.0" layoutY="269.0" fx:id="userNameField" />
      <Label layoutX="332.0" layoutY="329.0" prefHeight="30.0" prefWidth="107.0" text="nickname:" />
      <TextField layoutX="439.0" layoutY="329.0" fx:id="nickNameField" />
      <Label layoutX="332.0" layoutY="389.0" prefHeight="30.0" prefWidth="107.0" text="password:" />
      <TextField layoutX="439.0" layoutY="389.0" fx:id="passWordField" />
